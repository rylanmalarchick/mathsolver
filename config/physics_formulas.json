    {
      "id": "wien_displacement",
      "name": "Wien's Displacement Law",
      "category": "thermodynamics",
      "subcategory": "blackbody",
      "latex": "\\lambda_{max} T = b",
      "sympy_template": "lambda_max * T - b",
      "variables": ["lambda_max", "T"],
      "constants": {
        "b": {"value": 2.897771955e-3, "unit": "m*K", "name": "Wien's displacement constant"}
      },
      "units": {"lambda_max": "m", "T": "K"},
      "description": "Relates peak wavelength of blackbody radiation to temperature",
      "solve_templates": {
        "lambda_max": "b / T",
        "T": "b / lambda_max"
      },
      "steps": {
        "lambda_max": ["Starting with Wien's Law: λ_max · T = b", "Divide both sides by T", "λ_max = b / T"],
        "T": ["Starting with Wien's Law: λ_max · T = b", "Divide both sides by λ_max", "T = b / λ_max"]
      }
    },
